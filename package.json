{
  "name": "@xlts.dev/verdaccio-prometheus-middleware",
  "version": "0.1.0",
  "description": "Verdaccio middleware plugin used to collect prometheus compatible metrics",
  "keywords": [
    "verdaccio",
    "plugin",
    "middleware",
    "metrics",
    "prometheus"
  ],
  "author": {
    "name": "Ed Clement",
    "email": "ed@xlts.dev"
  },
  "license": "UNLICENSED",
  "homepage": "https://xlts.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/xlts-dev/verdaccio-prometheus-middleware"
  },
  "bugs": {
    "url": "https://github.com/xlts-dev/verdaccio-prometheus-middleware/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npm run build:types && npm run build:js",
    "build:js": "babel src/ --out-dir lib --extensions \".ts,.tsx\"",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "npm run lint && npm run unitTest",
    "unitTest": "jest",
    "lint": "eslint \"**/*.{js,ts}\""
  },
  "dependencies": {
    "express": "4.17.1",
    "prom-client": "14.0.1"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.9",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@verdaccio/babel-preset": "10.0.0",
    "@verdaccio/eslint-config": "10.0.0",
    "@verdaccio/types": "10.0.0",
    "chance": "1.1.8",
    "eslint": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-junit": "13.0.0",
    "prettier": "2.4.1",
    "typescript": "4.5.2"
  }
}
